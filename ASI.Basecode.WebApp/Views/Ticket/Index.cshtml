@model List<ASI.Basecode.Services.ServiceModels.TicketViewModel>

<h2>Tickets</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Ticket ID</th>
            <th>Subject</th>
            <th>Sender Email</th>
            <th>Created Time</th>
            <th>Updated Time</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var ticket in Model)
        {
            <tr>
                <td>@ticket.TicketId</td>
                <td>@ticket.Subject</td>
                <td>@ticket.SenderEmail</td>
                <td>@ticket.CreatedTime</td>
                <td>@ticket.UpdatedTime</td>
            </tr>
        }
    </tbody>

</table>